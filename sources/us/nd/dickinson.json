{
    "coverage": {
        "country": "us",
        "state": "nd",
        "city": "Dickinson"
    },
    "type": "ESRI",
    "data": "http://maps5.gisworkshop.com/ArcGIS/rest/services/CityofDickinsonElectric/MapServer/4",
    "conform": {
        "type": "geojson",
        "number": {
            "function": "regexp",
            "field": "PROPADDRESS",
            "pattern": "^(\S+)"
        },
        "street": {
            "function": "regexp",
            "field": "PROPADDRESS",
            "pattern": "^(?:\S+ )(.*)",
            "replace": "$1"
        }
    }
}