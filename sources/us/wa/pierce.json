{
  "coverage": {
    "US Census": {
      "geoid": "53053",
      "name": "Pierce County",
      "state": "Washington"
    },
    "country": "us",
    "state": "wa",
    "county": "Pierce"
  },
  "data": "https://services2.arcgis.com/1UvBaQ5y1ubjUPmd/arcgis/rest/services/Tax_Parcels/FeatureServer/0",
  "type": "ESRI",
  "conform": {
    "type": "geojson",
    "number": {
      "function": "regexp",
      "field": "Site_Address",
      "pattern": "^([0-9]+-?[A-Z]?) ",
      "replace": "$1"
    },
    "unit": {
      "function": "regexp",
      "field": "Site_Address",
      "pattern": " ((?:UNIT|APT|BLDG|LOT|#).*)$",
      "replace": "$1"
    },
    "street": {
      "function": "regexp",
      "field": "Site_Address",
      "pattern": "^(?:[0-9]+-?[A-Z]? )(.*?)((UNIT|APT|BLDG|LOT|#).*|$)",
      "replace": "$1"
    }
  }
}
