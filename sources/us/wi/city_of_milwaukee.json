{
    "coverage": {
        "country": "us",
        "state": "wi",
        "city": "Milwaukee"
    },
    "type": "ESRI",
    "data": "http://maps.milwaukee.gov/ArcGIS/rest/services/LMS/LMS_APO/MapServer/1",
    "conform": {
        "type": "geojson",
        "number": "HSE_NBR",
        "street": [
            "DIR",
            "STREET",
            "STTYPE"
        ],
        "postcode": {
            "function": "regexp",
            "field": "ZIP_CODE",
            "pattern": "^[\\d]{5}[\\d]{4}$",
            "replace": "$1-$2"
        },
        "city": "CITY"
    }
}
