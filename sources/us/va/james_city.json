{
    "coverage": {
        "US Census": {
            "geoid": "51095",
            "name": "James City County",
            "state": "Virginia"
        },
        "country": "us",
        "state": "va",
        "county": "James City"
    },
    "website": "https://www.jamescitycountyva.gov/assessments/gis-mapping-layers.html",
    "data": "ftp://property.jamescitycountyva.gov/GIS/layers/jcc_parcels.zip",
    "type": "ftp",
    "compression": "zip",
    "note": {
        "email": "propertyinfo@jamescitycountyva.gov",
        "phone": "757-253-6650",
        "metadata": "ftp://property.jamescitycountyva.gov/GIS/layers/GIS_Metadata.pdf"
    },
    "conform": {
        "number": {
            "function": "regexp",
            "field": "LocAddr",
            "pattern": "^([0-9]+)"
        },
        "street": {
            "function": "regexp",
            "field": "LocAddr",
            "pattern": "^(?:[0-9]+ )(.*)",
            "replace": "$1"
        },
        "postcode": {
            "function": "regexp",
            "field": "LocZip",
            "pattern": "^([0-9]{5})([0-9]{4})$",
            "replace": "$1-$2"
        },
        "type": "shapefile-polygon"
    }
}
