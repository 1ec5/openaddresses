{
    "coverage": {
        "ISO 3166": {
            "alpha2": "TW-TAO",
            "country": "Taiwan"
        },
        "country": "tw"
    },
    "layers": {
        "addresses": [
            {
                "type": "ESRI",
                "data": "http://gener.tycg.gov.tw/arcgis/rest/services/map/MapServer/18",
                "conform": {
                    "type": "geojson",
                    "number": {
                        "function": "regexp",
                        "field": "HOUSE_NO",
                        "pattern": "^([0-9０-９]+號?)"
                    },
                    "unit": {
                        "function": "regexp",
                        "field": "HOUSE_NO",
                        "pattern": "^(?:[0-9０-９]+號?)(.*)"
                    },
                    "street": {
                        "function": "join",
                        "fields": [
                            "STREET",
                            "SECT",
                            "LANE",
                            "ALLEY"
                        ],
                        "separator": ""
                    },
                    "city": "TOWNNAME",
                    "district": "LI",
                    "region": "COUNTY"
                }
            }
        ],
        "buildings": []
    }
}