{
    "coverage": {
        "ISO 3166": {
            "alpha2": "TW-TNN",
            "country": "Taiwan"
        },
        "country": "tw"
    },
    "layers": {
        "addresses": [
            {
                "type": "ESRI",
                "data": "http://water.tainan.gov.tw/tnwrbarcgis/rest/services/TNRW/Address/MapServer/0",
                "conform": {
                    "type": "geojson",
                    "number": {
                        "function": "regexp",
                        "field": "NO",
                        "pattern": "^([0-9０-９]+號?)"
                    },
                    "unit": {
                        "function": "regexp",
                        "field": "NO",
                        "pattern": "^(?:[0-9０-９]+號?)(.*)"
                    },
                    "street": {
                        "function": "join",
                        "fields": [
                            "ROADNAME",
                            "LANE"
                        ],
                        "separator": ""
                    },
                    "city": "VILLAGE",
                    "district": "DISTRICT",
                    "region": "COUNTY"
                }
            }
        ],
        "buildings": []
    }
}