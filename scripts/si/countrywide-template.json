{
    "website": "http://egp.gu.gov.si/egp/",
    "data": "https://s3.amazonaws.com/data.openaddresses.io/cache/si-%YYYY-MM-DD%.zip",
    "note": "Updated periodically. Source must be regenerated using script in this repo's /scripts/si directory.",
    "type": "http",
    "compression": "zip",
    "coverage": {
        "ISO 3166": {
            "alpha2": "SI",
            "country": "Slovenia"
        },
        "country": "si"
    },
    "license": {
        "text": "CC BY 2.5",
        "url": "https://creativecommons.org/licenses/by/2.5",
        "attribution": true,
        "attribution name": "Geodetska uprava Republike Slovenije, Register prostorskih enot, %YYYY-MM-DD%",
        "share-alike": false
    },
    "conform": {
        "encoding": "UTF-8",
        "srs": "EPSG:4326",
        "type": "csv",
        "csvsplit": ";",
        "lon": "lon",
        "lat": "lat",
        "number": "number",
        "street": "street",
        "city": "city",
        "district": "commune",
        "region": "region",
        "postcode": "postcode"
    }
}
